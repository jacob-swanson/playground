{
  "name": "@jswanson/js-utils",
  "version": "0.0.1-snapshot",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "Jacob Swanson <jacob.swanson.n@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/jest": "23.1.1",
    "barrelsby": "1.0.2",
    "jest": "23.1.0",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "typescript": "2.9.2"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib/* && tsc",
    "pretest": "yarn build",
    "test": "jest",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "generate-barrels": "barrelsby --delete --directory ./src"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/node": "10.3.5"
  }
}
